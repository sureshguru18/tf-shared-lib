#! /usr/bin/env groovy

def call(body) {
  def pipelineParams= [:]
  body.resolveStrategy = Closure.DELEGATE_FIRST
  body.delegate = pipelineParams
  body()

  pipeline {
    agent none
    options {
      ansiColor('xterm')
      disableConcurrentBuilds()
    }
    stages {
        stage('Running Terraform on CSW Account') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
}
